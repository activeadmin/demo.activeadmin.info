{
  "name": "demo.activeadmin.info",
  "description": "The demo application for Active Admin",
  "scripts": {
    "postdeploy": "bundle exec rails db:schema:load db:seed"
  },
  "env": {
    "AIRBRAKE_API_KEY": {
      "required": true
    },
    "AIRBRAKE_PROJECT_ID": {
      "required": true
    },
    "LANG": {
      "required": true,
      "value": "en_US.UTF-8"
    },
    "LIBRATO_PASSWORD": {
      "required": true
    },
    "LIBRATO_TOKEN": {
      "required": true
    },
    "LIBRATO_USER": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "RACK_ENV": {
      "required": true,
      "value": "production"
    },
    "RAILS_ENV": {
      "required": true,
      "value": "production"
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true,
      "value": "enabled"
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true,
      "value": "enabled"
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "airbrake",
    "librato",
    "papertrail",
    "heroku-postgresql",
    "scheduler"
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ],
  "stack": "heroku-18"
}
